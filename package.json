{
  "name": "ts-apollo-sequelize",
  "version": "0.0.1",
  "description": "Graphql Yoga Typescript Example",
  "main": "src/server.ts",
  "repository": "https://github.com/dooboolab/ts-graphql-yoga-example",
  "author": "dooboolab",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml",
    "codecov": "codecov",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:dev": "cross-env NODE_ENV=development sequelize db:migrate",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate",
    "migrate": "yarn migrate:dev && yarn migrate:prod"
  },
  "dependencies": {
    "apollo-server": "^2.9.16",
    "apollo-server-express": "^2.9.16",
    "bcrypt-nodejs": "^0.0.3",
    "graphql": "^14.6.0",
    "graphql-import": "^0.7.1",
    "graphql-subscriptions": "^1.1.0",
    "http": "^0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.1",
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-resolvers": "1.12.1",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.1.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^13.7.0",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "codecov": "^3.6.4",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "resolutions": {
    "graphql": "14.6.0"
  }
}
